
<div class="col s12 m8 l9">
  @ViewBag.NombreAccion

    <form class="col s12">
        <div class="row">
            <h4 class="center">Creación De Empresas</h4>
            <div class="input-field col s12 m6 l6">
                <i class="material-icons prefix">account_circle</i>
                <input id="icon_prefix" type="text" class="validate">
                <label for="icon_prefix">First Name</label>
            </div>
            <div class="input-field col s12 m6 l6">
                <i class="material-icons prefix">phone</i>
                <input id="icon_telephone" type="tel" class="validate">
                <label for="icon_telephone">Nit</label>
            </div>

            <div class="input-field col s12 m6 l6">
                <select class="browser-default">
                    <option>Paises</option>
                    <option value="1">Pais 1</option>
                    <option value="2">Pais 2</option>
                    <option value="3">Pais 3</option>
                </select>
            </div>


            <div class="input-field col s12 m6 l6">
                <select class="browser-default" disabled>
                    <option value="" disabled selected>Ciudad</option>
                    <option value="1">Option 1</option>
                    <option value="2">Option 2</option>
                    <option value="3">Option 3</option>
                </select>
            </div>

            <div class="input-field col s12 m6 l6">
                <i class="material-icons prefix">phone</i>
                <input id="icon_telephone" type="tel" class="validate">
                <label for="icon_telephone">Dirección</label>
            </div>

            <div class="input-field col s12 m6 l6">
                <i class="material-icons prefix">phone</i>
                <input id="icon_telephone" type="tel" class="validate">
                <label for="icon_telephone">Teléfono</label>
            </div>

            <div class="input-field col s12 m6 l6">
                <i class="material-icons prefix">email</i>
                <input id="icon_telephone" type="tel" class="validate">
                <label for="icon_telephone">Email-1</label>
            </div>

            <div class="input-field col s12 m6 l6">
                <i class="material-icons prefix">email</i>
                <input id="icon_telephone" type="tel" class="validate">
                <label for="icon_telephone">Email-2</label>
            </div>

            <div class="file-field input-field col s12 m6 l6">

                <div class="btn">
                    <span>File</span>
                    <input type="file" multiple>
                </div>
                <div class="file-path-wrapper">
                    <input class="file-path validate" type="text" placeholder="Upload one or more files">
                </div>

            </div>

            <div class="input-field col s12 m6 l6">
                <select class="browser-default">
                    <option>Colores</option>
                    <option class="card-panel light-blue darken-3"></option>
                    <option class="card-panel purple lighten-4"></option>
                    <option class="card-panel deep-orange accent-3"></option>
                </select>
            </div>

            <div class="input-field col s12 m6 l6">
                <div class="fixed-action-btn horizontal" style="bottom: 45px; right: 24px;">
                    <a class="btn-floating btn-large red">
                        <i class="large material-icons">menu</i>
                    </a>
                    <ul>
                        <li><a class="btn-floating red"><i class="material-icons">delete</i></a></li>
                        <li><a class="btn-floating green"><i class="material-icons">done</i></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </form>
</div>